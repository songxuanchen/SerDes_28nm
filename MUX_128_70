%% 定义变换矩阵
%左上角块矩阵，行列数相等，均等于dout数量，128行128列
mat_left_top = [zeros(1,128);eye(127,127),zeros(127,1)];

%右上角块矩阵，行列数不相等，行数等于dout数量，列数等于din数量，128行70列，元素全0
mat_right_top = zeros(128,70);

%左下角块矩阵，行列数不相等，行数等于din数量，列数等于dout数量-1，70行127列，元素为全0
mat_left_bottom = zeros(70,127);

%右下角块矩阵，行列数不相等，行数等于din数量，列数等于din数量+1，70行71列，元素每次都在变化
